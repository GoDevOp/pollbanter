<%= javascript_include_tag "//www.google.com/jsapi", "chartkick" %>

<p id="notice"><%= notice %></p>

<h1>Listing Polls</h1>

    <div class="row">
      <% @polls.each do |poll| %>
        <div class="col-xs-1 col-md-2"><%= link_to poll.title, poll %>
          <%= link_to(image_tag(poll.image.url(:thumb)), poll) %>        
        </div>
      <% end %>  
    </div>

        






<%= link_to 'New Poll', new_poll_path %>

